@inject SharedViewLocalizer Localizer

<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>@ViewData["Title"] - @Localizer["AppName"]</title>
	</head>
	<body>
		<main>
			@RenderBody()
		</main>
		<footer>
			<hr />
			<p>&copy; @DateTime.Now.Year Christopher Felegy / USF Big Data Analytics Lab </p>
			@await Html.PartialAsync("_CulturePartial")
		</footer>
	</body>
</html>